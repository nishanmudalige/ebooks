library(openintro)#
#
myPDF('tDistDF18LeftTail2Point10.pdf', 3, 1.7, mar=c(2,1,1,1), mgp=c(5,0.6,0))#
plot(c(-4, 4), c(0, dnorm(0)), type='n', axes=FALSE)#
axis(1)#
abline(h=0)#
#
COL <- c('#000000FF', '#00000091', '#0000006F', '#0000004B', '#00000022')#
DF <- c('normal', 10, 5, 2, 1)#
#
X <- seq(-5, 8, 0.01)#
Y <- dt(X, 10)#
lines(X, Y, col=COL[1])#
#
these <- which(X < -2.10)#
yy <- c(0, Y[these], 0)#
these <- c(these[1], these, rev(these)[1])#
xx <- X[these]#
polygon(xx, yy, col='#00000022', border='#000000AA')#
dev.off()
