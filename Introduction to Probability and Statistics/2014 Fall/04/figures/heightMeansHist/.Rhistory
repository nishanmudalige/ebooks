profit <- c(-110, -9, -60, 316, -200, -196, 320, -160, 31, 331, 1731, 21, -926, -475, 914, -300, -15, 1, -29, 829, 761, 227, -141, -672, 352, 385, 24, 103, -826, 95, 115, 39, -9, -1000, -35, -200, -200, 235, 70, 307, 135, 60, -100, -295, -1000, 361, -95, 337, 3712, -255)#
#
hold <- hist(profit, breaks=20)#
#
#===> plot <===##
par(mfrow=c(1,1), las=1, mgp=c(1.85,.6,0), mar=c(3,1,1,1))#
plot(0, 0, type='n', xlab='winnings (dollars)', ylab='', axes=F, main='',#
	xlim=range(hold$breaks), ylim=range(hold$density)*1.1)#
for(i in 1:length(hold$counts)){#
	rect(hold$breaks[i], 0, hold$breaks[i+1], hold$density[i],#
		 border='#222222', col='#BBCCDD')#
}#
axis(1, col='#BBBBBB', col.axis='#CCCCCC')#
axis(1, at=mean(profit), expression(mu), col.axis='#663300')#
axis(1, at=mean(profit)+sd(profit)*c(-1,1), expression(mu-sigma, mu+sigma), col.axis='#0077BB')#
axis(1, at=c(-100000, 100000))#
abline(v=mean(profit), col='#663300', lty=3)#
lines(rep(mean(profit)-sd(profit),2), c(0, max(hold$density)*.6), col='#0077BB', lty=3)#
lines(rep(mean(profit)+sd(profit),2), c(0, max(hold$density)*.6), col='#0077BB', lty=3)#
#
