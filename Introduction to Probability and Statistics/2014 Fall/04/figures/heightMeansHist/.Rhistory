	rect(hold5$breaks[i], 0, hold5$breaks[i+1], hold5$density[i],#
		 border='#222222', col='#BBCCDD')#
}#
axis(1)#
abline(h=0)#
text(670, max(hold5$density), 'samples of size 5')#
