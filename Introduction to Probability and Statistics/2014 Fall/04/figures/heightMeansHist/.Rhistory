hold45 <- hist(s45)#
#
par(mfrow=c(3,1), las=1, mgp=c(2,.7,0), mar=c(3,0,1,0))#
R <- range(hold5$breaks)#
plot(0, 0, type='n', xlab='male heights (in.)', ylab='', axes=F, main='',#
xlim=R, ylim=range(hold5$density)*1.1)#
