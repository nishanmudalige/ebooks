myPDF('hsMaleHeightsWPValue.pdf', 4, 2, mgp=c(3, 0.3, 0))#
#
#===> plot <===##
par(las=1, mar=c(2,0,0.5,0))#
X <- seq(-5,5,0.01)#
Y <- dt(X, 20)#
plot(X, Y, type='l', axes=F, xlim=c(-2.5,3.4))#
axis(1, at=c(-5, 0, 1.12, 5), label=expression(0, H[0]*': '*mu*' = 70.43  ', bar(x)*' = 71.8', 0), cex.axis=0.7)#
these <- which(X >= 1.12)#
polygon(c(X[these[1]], X[these],X[rev(these)[1]]), c(0,Y[these],0), col='#CCCCCC', border='#000000BB')#
yMax <- max(Y)#
#
text(0, yMax*.4, '0.889', cex=1)#
#
arrows(2, yMax*.6, 2, yMax/5, length=0.1)#
text(2, yMax*.6, 'p-value\n 0.111', pos=3, cex=1)#
#
#arrows(1.3,0.28, 0.43, 0.28, length=0.07)#
#text(-.88, 0.13, '  40%\n(0.40)', cex=0.8)#
#
lines(X, Y)#
abline(h=0)#
#
#
dev.off()
