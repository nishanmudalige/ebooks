pres <- read.delim("president.txt", TRUE)#
year  <- seq(1898, 2006, 4)+1#
n      <- length(year)#
unemp  <- rep(0, n)#
change <- rep(0, n)#
presid <- rep("", n)#
party  <- rep("", n)#
for(i in 1:n){#
	urow <- which(unempl$year == year[i]-1)#
	prow <- which(pres$yearEnd > year[i])[1]#
	hrow <- which(house$yearEnd >= year[i])[1]#
	party[i] <- as.character(pres$party[prow])#
	if(substr(house$g1[hrow],1,5) == substr(party[i],1,5)){#
		oldHouse <- house$ng1[hrow] / house$seats[hrow]#
	} else {#
		oldHouse <- house$ng2[hrow] / house$seats[hrow]#
	}#
	if(substr(house$g1[hrow+1],1,5) == substr(party[i],1,5)){#
		newHouse <- house$ng1[hrow+1] / house$seats[hrow+1]#
	} else {#
		newHouse <- house$ng2[hrow+1] / house$seats[hrow+1]#
	}#
	change[i] <- (newHouse - oldHouse)/oldHouse#
	presid[i] <- as.character(pres$potus[prow])#
	unemp[i]  <- unempl$unemp[urow]#
}#
#
unemployPres <- data.frame(year=year, potus=presid, party=party, unemp=unemp, change=change)#
#
these <- !year %in% c(1935, 1939)#
COL <- c("#0000CCAA", "#CC000066")#
plot(unemployPres$unemp[these], unemployPres$change[these],#
		col=COL[1+(unemployPres$party[these]=="Republican")],#
		pch=19, ylim=c(-0.3, 0.1), xlim=c(3, 12), axes=FALSE)#
axis(1, at=c(4, 8, 12))#
axis(2)#
cases <- c(1, 9, 13, 22, 26, 27)#
for(i in 1:length(cases)){#
	potus  <- as.character(unemployPres$potus[cases[i]])#
	potus  <- tail(strsplit(potus, " ")[[1]], 1)#
	year   <- unemployPres$year[cases[i]]#
	potus  <- paste(potus, "\n", year, sep="")#
	unem   <- unemployPres$unemp[cases[i]]#
	change <- unemployPres$change[cases[i]]#
	text(unem, change, potus, pos=3, cex=0.6)#
}#
#
summary(lm(change ~ unemp, unemployPres))#
#
summary(lm(change ~ unemp, unemployPres[these,]))
