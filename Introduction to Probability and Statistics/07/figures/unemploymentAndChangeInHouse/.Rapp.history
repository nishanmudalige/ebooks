change <- rep(0, n)#
presid <- rep("", n)#
party  <- rep("", n)#
for(i in 1:n){#
	urow <- which(unempl$year == year[i]-1)#
	prow <- which(pres$yearEnd > year[i])[1]#
	hrow <- which(house$yearEnd >= year[i])[1]#
	party[i] <- as.character(pres$party[prow])#
	if(substr(house$g1[hrow],1,5) == substr(party[i],1,5)){#
		oldHouse <- house$ng1[hrow] / house$seats[hrow]#
	} else {#
		oldHouse <- house$ng2[hrow] / house$seats[hrow]#
	}#
	if(substr(house$g1[hrow+1],1,5) == substr(party[i],1,5)){#
		newHouse <- house$ng1[hrow+1] / house$seats[hrow+1]#
	} else {#
		newHouse <- house$ng2[hrow+1] / house$seats[hrow+1]#
	}#
	change[i] <- (newHouse - oldHouse)/oldHouse#
	presid[i] <- as.character(pres$potus[prow])#
	unemp[i]  <- unempl$unemp[urow]#
}#
